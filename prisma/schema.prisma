// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Overview {
  id    Int     @id @default(autoincrement())
  title String  @unique
  slogan  String
  deskripsi String
  nomor_wa String
  text_chat_wa String
  label_wa String
}